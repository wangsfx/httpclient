<?xml version="1.0" encoding="UTF-8"?>
<Configration>
	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss} [%-5p] %C.%M:%L %m%n"/>
		</Console>
	</Appenders>
	<Loggers>
		<Root level="DEBUG" includeLocation="true">
			<AppenderRef ref="console"/>
		</Root>
	</Loggers>
</Configration>
